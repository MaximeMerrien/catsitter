<div class="container-fluid">
    <div class="row">
        <div class="col-4"></div>
        <div class="col-4">
            <h2>Contact</h2>
            <div>
                Adresse : <%= coords.address %>
            </div>

            <div>
                TÃ©l : <%= coords.tel %>
            </div>

            <div>
                Email : <a href="mailto:<%= coords.email %>"><%= coords.email %></a>
            </div>

            <hr />
            <form action="/contact" method="POST">
                <div class="form-group">
                    <label for="subject">Sujet</label>
                    <input
                        type="text"
                        name="subject"
                        id="subject"
                        class="form-control"
                    />
                </div>
                <div class="form-group">
                    <label for="message">Message</label>
                    <textarea
                        name="message"
                        id="message"
                        cols="30"
                        rows="10"
                        class="form-control">
                    </textarea>
                </div>
                    <input type="submit" value="Envoyer" class="btn btn-info btn-block" />
            </form>
        </div>
        <div class="col-4"></div>
    </div>
</div>